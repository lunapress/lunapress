{
    "name": "lunapress/cli",
    "description": "CLI for development",
    "type": "library",
    "bin": ["bin/lunapress"],
    "authors": [
        {
            "name": "Onepix",
            "homepage": "https://onepix.net",
            "role": "Company"
        },
        {
            "name": "0zd0",
            "role": "Lead Developer",
            "homepage": "https://github.com/0zd0"
        }
    ],
    "autoload": {
        "psr-4": {
